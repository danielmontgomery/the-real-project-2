<h1 class="titled"><i>The Top 100</i></h1>
<table class="myTable">
	<thead>
		<tr class="tableHeader">
			<th class="tableData1">Picture</th>
			<th class="tableData2">Points</th>
			<th class="tableData3">Caption</th>
		</tr>
	</thead>
	<tbody>
		<% @posts.each do |p| %>
		<tr class="tableHeader">
			<td class="tableData1"><%= image_tag p.pic.url(:medium) %></td>
			<td class="tableData22"><%= p.score %></td>
			<td class="tableData33"><%= p.caption %></td>
		</tr>
		<% end %>
	</tbody>
<%#= link_to "Cast your vote!", :url => destroy_multiple_pictures_path, :class => 'btn btn-primary' %>
